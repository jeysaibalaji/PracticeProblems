#include<stdio.h>
void main()
{
int a[6]={89,45,68,90,29,17};
int n=6;
insertionSort(a,n);
display(a,n);
}
void insertionSort(int a[],int n)
{ int i,j,item;
for (i=1; i<=n-1; ++i)
{
item = a[i];
for (j=i-1;j>=0; && a[j]>item; --j)
{
a[j+1]=a[j];
}
a[j+1]=item;
}
}
void display(int a[],int n)
{
int i;
for(i=0;i<=n-1; ++i)
{
printf("%d\t",a[i]);
}
}   

