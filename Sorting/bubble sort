#include<stdio.h>
void main()
{
int a[6] = {89,45,68,90,29,17};
int n = 6;
bubbSort(a,n);
display(a,n);
}
void bubbSort(int a[], int n)
{
int i,j,temp;
for(i=0; i<=n-2; ++i)
{
 for (j=0; j<=n-2-i; ++j)
 {
   if(a[j] > a[j+1])
   {
   temp = a[i];
   a[j] = a[j+1];
   a[j+1] = temp;
   }
   }
   }
   }
   void display(int a[] , int n)
   {
   int i;
   for (i=0; i<=n-1; ++i)
   {
        printf("%d\t",a[i]);
   }
   }
